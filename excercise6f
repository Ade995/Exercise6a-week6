x=input("enter a short sentence:")
y=input("enter a short sentence:")

setence=x+" "+y
print(sentence)

splitedSentence = sentence.split()
print(splitedSentence)

sortedSentence = sorted(splitedSentence)

print(sortedSentence)

print(len(sortedSentence))

from collections import  Counter
print(dict(Counter(splittedSentence)))
